<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>CreateLegalCustomerServlet</servlet-name>
        <servlet-class>presentationLayer.CreateLegalCustomerServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CreateRealCustomerServlet</servlet-name>
        <servlet-class>presentationLayer.CreateRealCustomerServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SearchLegalCustomerServlet</servlet-name>
        <servlet-class>presentationLayer.SearchLegalCustomerServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SearchRealCustomerServlet</servlet-name>
        <servlet-class>presentationLayer.SearchRealCustomerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CreateLegalCustomerServlet</servlet-name>
        <url-pattern>/CreateLegalCustomerServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CreateRealCustomerServlet</servlet-name>
        <url-pattern>/CreateRealCustomerServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SearchLegalCustomerServlet</servlet-name>
        <url-pattern>/SearchLegalCustomerServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SearchRealCustomerServlet</servlet-name>
        <url-pattern>/SearchRealCustomerServlet</url-pattern>
    </servlet-mapping>

</web-app>
